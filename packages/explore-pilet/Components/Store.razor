<li class="e_Store">
  <div class="e_Store_content">
    <img class="e_Store_image" src=@Utils.Src(Image, 200) src-set=@Utils.SrcSet(Image, [200, 400]) width="200" height="200" />
    <p class="e_Store_address">
      @Name
      <br />
      @Street
      <br />
      @City
    </p>
  </div>
</li>
    
@code {
    [Parameter]
    public string Image { get; set; }
    
    [Parameter]
    public string Street { get; set; }
    
    [Parameter]
    public string Name { get; set; }
    
    [Parameter]
    public string City { get; set; }
}